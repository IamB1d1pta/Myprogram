import java.util.*;
public class array{
    public static void main(String args[])
    {
	Inp h=new Inp();
	
	
 for(int i=0;i<size;i++)
        {
            System.out.println("\t");
            System.out.println(h.numbers[i]+5);
            
        }
        
    }
}
class Inp{
Scanner sc=new Scanner(System.in);
        int size=sc.nextInt();
        int numbers[]=new int[size];
        for(int i=0;i<size;i++)
        {
            numbers[i]=sc.nextInt();
            
        }

}
